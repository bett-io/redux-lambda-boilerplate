{
  "name": "helloworld-lambda-web",
  "version": "1.0.0",
  "description": "Helloworld on lambda for a web",
  "main": "index.js",
  "scripts": {
    "test": "mocha-webpack --webpack-config webpack.test.config.js test",
    "start": "webpack-dev-server --config webpack.client.config.js --inline --content-base ./devserver/ --history-api-fallback",
    "start:standalone": "npm run build:standalone && node ./standalone/standalone.bundle.js",
    "build:client": "NODE_ENV=production webpack --config webpack.client.config.js",
    "build:server": "NODE_ENV=production webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "build:standalone:client": "webpack --config webpack.standalone.client.config.js",
    "build:standalone:server": "webpack --config webpack.standalone.server.config.js",
    "build:standalone": "npm run build:standalone:client && npm run build:standalone:server",
    "dist": "cp package.json ./dist/ && cd ./dist && npm install --production",
    "lint": "eslint . --cache",
    "deploy": "node-lambda deploy -p --description `git rev-parse HEAD` --prebuiltDirectory dist",
    "belazy": "npm run lint && npm run build && npm run dist && npm run deploy"
  },
  "author": "",
  "dependencies": {
    "aws-sdk": "^2.49.0",
    "aws-serverless-express": "^2.1.3",
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "connect-dynamodb": "^1.0.12",
    "express": "^4.14.1",
    "express-session": "^1.15.5",
    "prop-types": "^15.5.9",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.10.0",
    "if-env": "^1.0.0",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-lambda": "^0.9.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "style-loader": "^0.16.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
